@model Umbraco.Web.PublishedModels.TwoColumnsTextLeft

<div class="row content-panel content-panel-text-image" style="background-image:url('@(Model.Image != null ? Model.Image.Url : "")')">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-md-6">
                <h1>@Model.Title</h1>
                @Html.Raw(Model.Contents)
            </div>
        </div>
    </div>
</div>
